; @(#)lion1.mat
;
; Full geocoding match rules with left and right zip codes
;
VAR XStreetL   1   10  X ; Cross Street
VAR LeftFrom   11  10  X ; Left from house number
VAR LeftTo     21  10  X ; Left to house number
VAR XStreetR   31  10  X ; Cross Street
VAR RightFrom  41  10  X ; Right from house number
VAR RightTo    51  10  X ; Right to house number
VAR PreDir     61   2  X ; Prefix direction
VAR PreType    63   6  X ; Prefix street type
VAR StreetName 69  30  S ; Street name
VAR StreetType 99   6  X ; Suffix street type
VAR SufDir     105  2  X ; Suffix direction
VAR LeftZone   107 30  X ; Left zone
VAR RightZone  137 30  X ; Right zone
;
MATCH INTERVAL_NOPAR CS XStreetL XStreetR 0.999 0.01 ZERO_VALID
MATCH LR_UNCERT ZN LeftZone RightZone 0.7 0.1 700.0 EITHER
MATCH UNCERT  SN StreetName 0.9 0.01 700.0
MATCH CHAR    PD PreDir     0.8 0.1
MATCH CHAR    PT PreType    0.7 0.1
MATCH CHAR    ST StreetType 0.85 0.1
MATCH CHAR    SD SufDir     0.85 0.1
MATCH D_INT   HN LeftFrom LeftTo RightFrom RightTo 0.7 0.1 ZERO_VALID
;
VARTYPE LeftFrom NOFREQ
